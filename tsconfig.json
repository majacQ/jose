{
  "files": ["./src/index.ts"],
  "compilerOptions": {
    "outDir": "./dist/webapi",
    "target": "ES2022",
    "module": "ES2022",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "types": [],
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": false,
    "removeComments": true,
    "verbatimModuleSyntax": true
  }
}
